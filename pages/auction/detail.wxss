#auction-detail {
  background: #f0f0f0;
  min-height: 100%;
  padding-bottom: 100rpx;
}

.auction-banner {
  height: 300rpx;
}

.auction-banner image {
  width: 100%;
  height: 100%;
}

.auction-detail-1 {
  padding: 25rpx 50rpx 35rpx 50rpx;
  background: #fff;
  position: relative;
  overflow: hidden;
}

.auction-detail-1 .auction-number-title {
  padding-right: 20rpx;
}

.auction-detail-1  .auction-price {
  padding-top: 25rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.auction-detail-1 .text {
  color: #666;
  font-size: 24rpx;
  width: 85rpx;
  text-align: left;
}

.auction-detail-1  .price {
  font-size: 48rpx;
  flex: 1;
}
.red-color{
  color: #FE7073;
  padding-left: 20rpx;
  font-size:34rpx;
}

.auction-time-resource {
  color: #666;
  font-size: 24rpx;
  display: flex;
  flex-direction: row;
  padding-top: 15rpx;
}
.auction-time-info{
  font-size: 26rpx;
  margin-left: 10rpx;
}
.time, .resource {
  flex: 1;
}

.text-right{
  text-align: right;
}

.book-btn {
  font-size: 30rpx;
  position: absolute;
  right: 0;
  top: 50rpx;
  color: #fff;
  padding: 30rpx 15rpx;
  background: #fe7073;
  width: 60rpx;
  word-wrap: wrap;
}

.auction-detail-2 {
  display: flex;
  margin-top: 10rpx;
  background: #fff;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: flex-start;
  align-items: center;
}

.auction-detail-4{
  margin-top: 10rpx;
  background: #fff;
}
.auction-detail-3 {
  background: #fff;
}

.table-cell {
  width: 50%;
  border-bottom: 1px solid #ccc;
  display: flex;
  flex-direction: row;
}

.cell-text {
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  color: #666;
  font-size: 24rpx;
  padding-left: 50rpx;
  padding-right: 24rpx;
}

.cell-value {
  padding: 20rpx;
  color: #666;
  font-size: 26rpx;
  flex: 1;
}


.border-right-none {
  border-right: 0 none;
}

.border-bottom-none {
  border-bottom: 0 none;
}

.row {
  border-bottom: 1px solid #ccc;
  padding: 30rpx 50rpx;
  height: 30rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.text {
  color: #333;
  font-size: 30rpx;
}

.value {
  color: #666;
  font-size: 24rpx;
  line-height: 30rpx;
  flex: 1;
  text-align: right;
  padding-right: 20rpx;
}

.icon-location {
  transform: rotate(360deg);
  width: 14rpx;
  height: 28rpx;
  font-size: 28rpx;
}

.row:last-child {
  border-bottom: 0 none;
}

.row:first-child .value {
  text-align: left;
  padding-left: 0.2rem;
}

.auction-detail-4 .title, .auction-detail-5 .title, .auction-detail-6 .title {
  background: #fff;
  font-size: 30rpx;
  height: 30rpx;
  line-height: 1;
  padding: 25rpx 0;
  text-align: center;
}

.auction-detail-6 image {
  width: 100%;
}

.auction-detail-4 {
  padding: 10rpx;
}

.table {
  display: grid;
  text-align: center;
  grid-template-columns: 175rpx 212rpx auto;
  border-left: 1px solid #ccc;
  border-top: 1px solid #ccc;
  margin-top: 10rpx;
}

.cell {
  padding: 15rpx 0;
  border-bottom: 1px solid #ccc;
  border-right: 1px solid #ccc;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
}

.grid-column-24 {
  grid-column: 2/4;
}

.border-bottom-none {
  border-top: 0 none;
}

.auction-detail-5 .video {
  height: 300rpx;
  width: 100%;
  display: block;
  margin: 10rpx auto;
}

.auction-detail-5 .pic-list {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  align-items: center;
  padding-right: 1%;
}

.auction-detail-5 .pic-list .pic-item {
  width: 49%;
  height: 300rpx;
  margin: 5rpx 0;
  margin-left: 1%;
}

.auction-detail-5 .pic-list .pic-item image {
  width: 100%;
  height: 100%;
  border-radius: 8rpx;
}

.book-btn-bottom {
  display: flex;
  z-index: 1;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  font-size: 32rpx;
  line-height: 60rpx;
  box-shadow: 0 0 25px #cac6c6;
}

.phone, .btn {
  flex: 1;
  height: 60rpx;
  padding: 20rpx 0;
  align-items: center;
  justify-content: center;
  text-align: center;
  display: flex;
}

.phone {
  background: #fff;
}

.phone .phone-icon,.eye-icon {
  width: 60rpx;
  height: 60rpx;
  margin-right: 20rpx;
  border-radius: 100%;
}

.btn {
  color: #fff;
  background: linear-gradient(to right, #3eb675, #07c1ab);
}

.icon-phone {
  border: 1px solid #07c1ab;
  border-radius: 50%;
  color: #07c1ab;
}

.dialog-mask {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 1000;
  background: rgba(0, 0, 0, 0.8);
}

.dialog {
  top: 50%;
  right: auto;
  bottom: auto;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  position: absolute;
  z-index: 1000;
  max-width: 100%;
  max-height: 100%;
  width: 600rpx;
}

.dialog-header {
  padding: 35rpx 0;
  text-align: center;
  color: #333;
  border-radius: 5px 5px 0 0;
  font-size: 36rpx;
  background: #cdcdcd;
  height: 50rpx;
  line-height: 50rpx;
}

.dialog-body {
  background: #fff;
  height: 500rpx;
  overflow: scroll;
}

.dialog-footer {
  height: 40rpx;
  padding: 20rpx 0;
  background: #1abd98;
  color: #fff;
  font-size: 30rpx;
  text-align: center;
  border-radius: 0 0 5px 5px;
}

.dialog-footer button {
  background: transparent;
  height: 40rpx;
  line-height: 40rpx;
  color: #fff;
  border: 0 none;
}

.dialog .input-item {
  display: flex;
  height: 50rpx;
  padding: 25rpx 0;
  border-bottom: 1px solid #ccc;
}

.dialog input {
  flex: 1;
  font-size: 30rpx;
  height: 50rpx;
  line-height: 50rpx;
  outline: 0 none;
  color: #333;
  padding-left: 30rpx;
}

.dialog .see-rule {
  font-size: 30rpx;
  color: #333;
  height: 50rpx;
  line-height: 50rpx;
}

.dialog .switch {
  height: 50rpx;
}

.dialog-close {
  color: #fff;
  background: transparent;
  height: 82rpx;
  margin-top: 20rpx;
}

.dialog-close .close {
  width: 78rpx;
  height: 78rpx;
  line-height: 78rpx;
  border-radius: 50%;
  text-align: center;
  font-size: 78rpx;
  border: 2rpx solid #fff;
  margin: 0 auto;
}

.sm-code-btn {
  width: 250rpx;
  color: #1abd98;
  font-size: 30rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: left;
  padding-left: 20rpx;
  border-left: 1px solid #ccc;
}

.view-table {
  width: 100%;
}

.view-table-tr {
  display: flex;
  flex-direction: row;
  border-bottom: 1px solid #ccc;
}

.view-table-tr:last-child {
  border-bottom: 0 none;
}

.view-table-td:first-child {
  width: 212rpx;
}

.view-table-td:last-child {
  border: 0 none;
  flex: 1;
}

.view-table-td {
  padding: 15rpx 0;
  border-right: 1px solid #ccc;
}

.fade-in {
  animation: fade-in ease-in 0.2s;
}

.fade-out {
  animation: fade-out ease-out 0.2s;
}

@keyframes fade-in {
  0% {
    opacity: 0;
  }

  100% {
    opacity: 1;
  }
}

@keyframes fade-out {
  0% {
    opacity: 0;
  }

  100% {
    opacity: 1;
  }
}

.select {
  position: relative;
  flex: 1;
  height: 100%;
  padding: 0 30rpx;
  font-size: 30rpx;
  text-align: left;
}

.select .val {
  font-size: 30rpx;
}

.select .val:after {
  content: '';
  position: absolute;
  right: 20rpx;
  top: 50%;
  width: 0;
  height: 0;
  margin-top: -10rpx;
  border-left: 20rpx solid transparent;
  border-right: 20rpx solid transparent;
  border-top: 20rpx solid #666;
  border-radius: 10rpx;
  transition: transform 0.3s cubic-bezier(0.86, 0, 0.07, 1);
}

.options {
  position: absolute;
  left: 0;
  top: 0rpx;
  right: 0;
  z-index: 5;
  background: #fff;
  box-shadow: 0 0 5px #ccc;
}

.options .option {
  padding: 10rpx 30rpx;
  border-bottom: 1px solid #ccc;
}

.hover-class {
  background: #f0f0f0;
}

.icon-phone {
  margin-right: 18rpx;
}

.detail-icon {
  width: 55rpx;
  height: 55rpx;
  vertical-align: middle;
  margin-right: 18rpx;
}

.panorama-img {
	height: 150rpx;
}
.panorama-img image{
  height: 100%;
  width: 100%;
}

.map-icon{
  width: 26rpx;
  height: 30rpx;
  vertical-align: middle;
}

.auction-banner .video {
  height:300rpx;
  width:100%;
  display:block;
  margin:10rpx auto;

}
.auction-end-icon {
  position:absolute;
  width:159rpx;
  height:129rpx;
  right:50rpx;
  top:110rpx;
}

.auction-detail-1 .gray-style{
  color: #cdcdcd;
  font-size: 38rpx;
}

