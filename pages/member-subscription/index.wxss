.member_subscription {
  height: 100%;
}
page{
  height: 100%;
  background: #f0f0f0;
  font-size: 28rpx;
}
.member_subscription_m{
  background:#fff;
  margin:15rpx;
  padding:20rpx;
  border-radius:12rpx;
  display: flex;
  font-size: 28rpx;
  align-items: center;
}
.m_s_detal{
  flex: 1;
  line-height: 48rpx;
}
.m_s_c{
  font-size: 24rpx;
  color: rgb(131, 130, 130);
}
.m_s_cancel{
  border:1px solid rgb(241, 141, 141);
  color:rgb(241, 141, 141);
  padding:12rpx 20rpx;
  border-radius:14rpx;
  font-size:26rpx;
}
.add_m_s{
  position: fixed;
  bottom: 60rpx;
  width: 100%;
}
.add_m_s_b{
  width: 70%;
  margin: auto;
  background: linear-gradient(to right, #3eb675, #07c1ab);
  color: #fff;
  border-radius: 50rpx;
  font-size: 28rpx;
  padding: 20rpx;
  text-align: center;
  letter-spacing:1rpx;
}